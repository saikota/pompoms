{
  "manifest_version": 2,
  "name": "My Cool Chrome Extension",
  "version": "0.1",
  "browser_action": {
  "default_icon": "icon.png",
  "default_title": "Kawa Panga"
  },
  "background": {
  "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.1.4.min.js","content.js"]
  }
]
}